@page "/counter"
@rendermode InteractiveServer



<h1>Counter</h1>

<p role="status"> @currentCount</p>

<button class="btn btn-primary" @onclick="IncrementCount">Click</button>

@code {
    private int currentCount = 0;

    private void IncrementCount()
    {
        currentCount++;
        if(currentCount > 10){
            currentCount = 0;
        }
    }
 
}
